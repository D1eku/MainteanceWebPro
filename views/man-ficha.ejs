    <script>
        
        const pautaInfo = <%- JSON.stringify(pautaInfo) %>

        function fillPage(data){
            console.log(data)
            document.getElementById('mant-code-pauta').value = data.codigoPautaMantencion
            document.getElementById('name-pauta').value = data.nombrePauta
            document.getElementById('tec-name-pauta').value = data.nombreTecnicoPauta
            document.getElementById('name-equipo-pauta').value = data.nombreEquipoPauta
            document.getElementById('code-equipo-pauta').value = data.codigoEquipoPauta
            document.getElementById('date-cal-pauta').value = data.fechaCalendarizadaPauta
            document.getElementById('date-rea-pauta').value = new Date(Date.now()).toLocaleDateString()
            
        }

        fillPage(pautaInfo);
        
        







